extends layout/main.pug


//- include ../blocks/sArticles/_sArticles.pug
//- include ../blocks/sOffers/_sOffers.pug
include ../blocks/sDelivery/_sDelivery.pug
include ../blocks/sDeliveryMethods/_sDeliveryMethods.pug 
include ../blocks/sContact/_sContact.pug

block variables
	- let  mainPage= null 
	- let  bg= '12.png'

block title
	- let  title= 'Доставка'
	title= title
block pagecontent
	//- +mob-mnu()
	+header()
		+topLine('cart')
		+top-nav()
	+breadcrumb('Главная', title).mb-0
	+sDelivery()
	+sDeliveryMethods()
	+sForm(`Нужна помощь специалиста?`)
	+sContact()
	+footer()



