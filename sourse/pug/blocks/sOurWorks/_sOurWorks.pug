mixin sOurWorks(data)
	// start sOurWorks
	+b.SECTION.sOurWorks.section#sOurWorks&attributes(attributes)
		.container
			+b.section-title
				h1 Наши работы
			.row
				- for (var x=0;x<4;x++)
					.col-lg-6
						+work-example(1,`ЖК Садовые кварталы`,`Плиты 1200*600*40, колотые, 2000 м2 для фасада.`)
					.col-lg-6
						+work-example(2,`Дудергофский канал`,`Гранитные ступени.`)
			+e.show
				+e.show-title(href="#") Показать еще XX
					+icon('plus-load')
			+pugin()
	// end sOurWorks